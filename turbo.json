{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["eslint.config.js"],
  "tasks": {
    "lint": {
      "dependsOn": ["^build"]
    },
    "format": {},
    "format:check": {},
    "types": {
      "dependsOn": ["^build"]
    },
    "test": {},
    "test:watch": {},
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  }
}
